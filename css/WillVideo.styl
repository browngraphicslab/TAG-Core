@import 'common.styl'

#WillVideoRoot
	wh 100%
	overflow-x hidden	

#WillVideoRoot #touchToExplore
	position relative
	top 10%
	width 100%
	height 20%
	z-index 2
	color #d99b3b
	font-weight 900
	text-align center
	font-size 36pt

#WillVideoRoot #videoContainer
	background-color black
	position absolute
	width 100%
	height auto
	top 0%
	text-align center
	z-index 0

#WillVideoRoot #video
	position relative
	width 100%
	max-width 100%
	max-height 100%






